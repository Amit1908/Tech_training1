package com.techment.debugging;

class Employee
{
	int id;
	String name;
	public Employee(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	
	String display()
	{
		return "id "+ id +" name "+ name;
	}
	
	
}

public class EmployeeDemo {

	public static void main(String[] args) {
		

	}

}
