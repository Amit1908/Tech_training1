<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 
               
     


<bean id="r" class="com.techment.model.Resturant">

<property name="resturantName" value="Zaika" ></property>
<property name="hotDrink" ref="c"></property>

<property name="softDrink" ref="mazza"></property>



</bean> 
<bean id="t" class="com.techment.model.Tea">
<property name="price" value="10"></property>
</bean>    
<bean id="c" class="com.techment.model.coffee">

</bean>    
<bean id="m" class="com.techment.model.Milk">

</bean>    
<bean id="p" class="com.techment.model.Pepsi">

</bean>    
<bean id="mazza" class="com.techment.model.Mazza">

</bean>    


               
</beans>